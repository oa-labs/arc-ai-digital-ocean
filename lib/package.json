{
  "name": "@arc-ai/shared",
  "version": "1.0.0",
  "description": "Shared modules for ArcAI  projects",
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
