  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.913.0",
    "@aws-sdk/s3-request-presigner": "^3.913.0",
    "@supabase/supabase-js": "^2.75.0",
    "dotenv": "^17.2.3",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.19",
    "typescript": "^5.9.3"
  }
}
