{
  "name": "@ichat-ocean/cli",
  "version": "1.0.0",
  "description": "Command line tool for interacting with the iChat Ocean agent service",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "ichat-cli": "./bin/ichat-cli.js"
  },
  "files": [
    "dist/",
    "lib/",
    "bin/",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && node ./scripts/sync-lib.js && node ./scripts/create-bin.js",
    "dev": "tsc --watch",
    "clean": "rm -rf dist bin",
    "type-check": "tsc --noEmit",
    "start": "node ./dist/index.js",
    "install": "npm --prefix ./lib install",
    "prepublishOnly": "npm run build",
    "lint": "echo 'No linter configured'"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.9.3"
  }
}
